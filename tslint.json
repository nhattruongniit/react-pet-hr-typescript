{
  "extends": ["tslint:recommended", "tslint-react", "tslint-no-unused"],
  "rules": {
    "interface-name": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "quotemark": false,
    "ordered-imports": false,
    "react-hooks/exhaustive-deps": false,
    "jsx-no-multiline-js": false,
    "no-unused": true
  },
  "jsRules": {},
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["bin", "src/serviceWorker.ts"]
  }
}
